def tail_list_comprehension(xs):
    return [x for i, x in enumerate(xs) if i != 0]

def tail(xs):
    return xs[1:]

def test_tail(tail):
    assert tail([1, 2]) == [2]
    assert tail([1]) == []
    assert tail([1, 2, 3, 4]) == [2, 3, 4]
    # f-string
    print(f'{tail.__name__} passed!')
    # str.format
    print('{} passed!'.format(tail.__name__))
    # str.format with keywords
    print('{n} passed!'.format(n=tail.__name__))
    # Percent formatting
    print('%s passed!' % (tail.__name__))

test_tail(tail)
test_tail(tail_list_comprehension)

def use_tee():
    from itertools import tee
    tee("hey", 10)
    iterators = tee("hey", 10)
     for it in iterators:
         for x in it:
             print("{} el in {} iterator".format(x, it))
